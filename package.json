{
  "name": "ngx-libraries",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --open",
    "build:flatpickr": "ng build ngx-flatpickr --prod",
    "publish:flatpickr": "npm run build:flatpickr && cd dist/ngx-flatpickr && npm publish --access=public",
    "build:timepicker": "ng build ngx-timepicker --prod",
    "publish:timepicker": "npm run build:timepicker && cd dist/ngx-timepicker && npm publish --access=public",
    "build:rights": "ng build ngx-rights --prod",
    "publish:rights": "npm run build:rights && cd dist/ngx-rights && npm publish --access=public",
    "build:avatar": "ng build ngx-avatar --prod",
    "publish:avatar": "npm run build:avatar && cd dist/ngx-avatar && npm publish --access=public",
    "build:roles": "ng build ngx-roles --prod",
    "publish:roles": "npm run build:roles && cd dist/ngx-roles && npm publish --access=public",
    "build:trumbowyg": "ng build ngx-trumbowyg --prod",
    "publish:trumbowyg": "npm run build:trumbowyg && cd dist/ngx-trumbowyg && npm publish --access=public",
    "build:forms": "ng build ngx-forms --prod",
    "publish:forms": "npm run build:forms && cd dist/ngx-forms && npm publish --access=public",
    "build:select": "ng build ngx-mat-select",
    "publish:select": "npm run build:select && cd dist/ngx-mat-select && npm publish --access=public",
    "publish:all": "npm run publish:flatpickr && npm run publish:timepicker && npm run publish:rights && npm run publish:avatar && npm run publish:roles && npm run publish:trumbowyg && npm run publish:forms && npm run publish:select",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
